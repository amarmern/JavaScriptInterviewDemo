//console.log("hello")
//0,1,1,2,3,5,8,13...

const fibnocci = (num) =>{
if(num===1){
return 0;
}
if(num ===2){
return 1
}
return fibnocci(num-1) + fibnocci(num-2)
}
console.log(fibnocci(5)) 

const fibbonnaciSeries = (num) =>{
let fib = [0,1];
//if(num <2) return 1;
for(let i =2; i < num; i++){
fib[i] = fib[i-1] + fib[i-2]
}
console.log("fibseries" ,fib)
}

fibbonnaciSeries(8)






